{
  "Cors": {
      "AllowAll": true,
      "Origins": []
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Trace",
      "Microsoft": "Trace",
      "Mbs": "Trace"
    }
  },
  "Serilog": {
    // Verbose (Trace), Debug, Information, Warning, Error, Fatal (Critical)
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose",
        "Mbs": "Verbose"
      }
    }
  }
}
