{
  // "urls": "http://localhost:5000;https://localhost:5001", // do not forget a trailing slash
  "EnableSwagger": true,
  "Properties": {
    "Application": "Mbs.Api.Host.Ng"
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowAll": true,
    "Origins": []
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error",
      "Mbs": "Error",
      "System": "Error",
      "Microsoft": "Error"
    }
  }
}
